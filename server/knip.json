{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["index.ts"],
  "project": ["**/*.ts"],
  "ignore": [
    "dist/**",
    "node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "prisma/migrations/**"
  ],
  "ignoreDependencies": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "ignoreExportsUsedInFile": true
}
